<div class="container d-flex flex-column gap-2 custom-margin-form">
<div >
  <h1 >Editing Ambassador</h1>
</div>
  <div class="d-flex justify-content-end gap-2" style="margin-right:20px;">
    <%= link_to @ambassador, class: "btn btn-secondary custom-button btn-icon-text" do %>
      <i class="fas fa-eye fa-icon" data-text="Show Ambassador"></i> 
     <% end %>  |
    <%= link_to ambassadors_path, class: "btn btn-secondary custom-button btn-icon-text" do %>
        <i class="fas fa-arrow-left fa-icon" data-text="Go Back"></i> 
      <% end %>
</div>
<div>
<%= render "form", ambassador: @ambassador %>
</div>
</div>



